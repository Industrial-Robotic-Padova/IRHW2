<launch>
  <node pkg="tiago_iaslab_simulation" name="pose_server" type="pose_server.py" output="screen"></node>
  <node pkg="tiago_iaslab_simulation" name="pose_client_test" type="pose_client_test.py" output="screen"></node>

  <include file="$(find tiago_iaslab_simulation)/launch/apriltag.launch"/>
  <node pkg="tiago_iaslab_simulation" name="node_detect" type="node_detect.py" output="screen"></node>

  <node pkg="tiago_iaslab_simulation" name="human_node" type="human_node"/>

  <node pkg="tiago_iaslab_simulation" name="node_pick_place" type="node_pick_place.py" output="screen">
      <param name="object_width"  value="0.05" />
      <param name="object_height" value="0.1" />
      <param name="object_depth"  value="0.05" />
  </node>
</launch>
